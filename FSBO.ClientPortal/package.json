  "scripts": {
    "postinstall": "node node_modules/webpack/bin/webpack.js --config webpack.config.vendor.js",
    "prune": "prune",
    "test": "karma start --reporters dots",
    "webpack-watch": "webpack --watch",
    "webpack-app:dev": "webpack --config webpack.config.js",
    "webpack-app:prod": "webpack --config webpack.config.js --env.prod",
    "webpack-app-with-stats:dev": "webpack --config webpack.config.js --profile --json > stats-app.dev.json",
    "webpack-app-with-stats:prod": "webpack --config webpack.config.js --profile --env.prod --json > stats-app.prod.json",
    "webpack-vendor:dev": "webpack --config webpack.config.vendor.js",
    "webpack-vendor:prod": "webpack --config webpack.config.vendor.js --env.prod",
    "webpack-vendor-with-stats:dev": "webpack --config webpack.config.vendor.js --profile --json > stats-vendor.dev.json",
    "webpack-vendor-with-stats:prod": "webpack --config webpack.config.vendor.js --env.prod --profile --env.prod --json > stats-vendor.prod.json",
    "webpack-full:dev": "webpack --config webpack.config.vendor.js && webpack --config webpack.config.js",
    "webpack-fulls:prod": "webpack --config webpack.config.vendor.js --env.prod && webpack --config webpack.config.js --env.prod",
    "webpack-full-with-stats:dev": "webpack --config webpack.config.vendor.js --profile --json > stats-vendor.dev.json && webpack --config webpack.config.js --profile --json > stats-app.dev.json",
    "webpack-full-with-stats:prod": "webpack --config webpack.config.vendor.js --env.prod --profile --json > stats-vendor.prod.json && webpack --config webpack.config.js --env.prod --profile --json > stats-app.prod.json"
  }
